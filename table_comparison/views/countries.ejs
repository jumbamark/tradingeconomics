<!DOCTYPE html>
<html>

<head>
    <title>Trading Economics Data Table</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
        }

        th,
        td {
            padding: 8px;
            text-align: left;
            border-bottom: 1px solid #ddd;
        }

        tr:hover {
            background-color: #f5f5f5;
        }
    </style>
</head>

<body>
    <h1>Trading Economics API Data:</h1>
    <ul>
        <% apiData.forEach(function(country) { %>
            <li>
                <h3>Country: <%= country.Country %>
                </h3>
                <p>Continent: <%= country.Continent %>
                </p>
                <p>Group: <%= country.Group %>
                </p>
                <p>ISO3: <%= country.ISO3 %>
                </p>
                <p>ISO2: <%= country.ISO2 %>
                </p>
            </li>
            <% }); %>
    </ul>

    <h1>Data from API</h1>
    <% if (apiData) { %>
        <!-- Display the API data -->
        <pre><%= JSON.stringify(apiData, null, 2) %></pre>
        <% } else { %>
            <p>No data available</p>
            <% } %>

                <script src="./js/script.js"></script>
</body>

</html>